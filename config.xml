<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.edu.unlp.lidi.asistenciaBluetooth" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Appsistencia</name>
    <description>
    App para pasar asistencia de forma automatica via bluetooth
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
       Cuiti√±o, Alfonso
       Fernandez Sosa, Juan Francisco
       Lasarte, Julia 
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
